{
	"name":			"ronnyjss/traducao",
	"description":	"Biblioteca para importar gettext de arquivos PO.",
	"homepage":		"https://github.com/ronnyjss/traducao",
	"keywords":		["gettext", "i18n", "translation", "po"],
	"license":		"MIT",
	"type":			"library",
	"authors": [{
		"name":		"RonnyJSS",
		"role":		"Developer",
		"email":	"ronny@jss.art.br",
		"homepage":	"https://github.com/ronnyjss"
	}],
    "support": {
        "email":	"ronny@jss.art.br",
        "issues":	"https://github.com/ronnyjss/traducao/issues"
    },
	"require": {
		"php":				"^8.0",
		"gettext/gettext":	"^5.6"
	},
    "require-dev": {
		"phpunit/phpunit": "^9.0"
    },
	"autoload": {
		"psr-4": {
			"ronnyjss\\": "src"
		},
		"files": [
			"src/constante.php",
			"src/funcao.php",
			"src/inicializar.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"ronnyjss\\traducao\\teste\\": "tests"
		}
	},
	"scripts": {
		"php56": [
			"/usr/local/bin/ea-php56 /opt/cpanel/composer/bin/composer"
		],
		"php71": [
			"/usr/local/bin/ea-php71 /opt/cpanel/composer/bin/composer"
		],
		"php72": [
			"/usr/local/bin/ea-php72 /opt/cpanel/composer/bin/composer"
		],
		"php73": [
			"/usr/local/bin/ea-php73 /opt/cpanel/composer/bin/composer"
		],
		"php74": [
			"/usr/local/bin/ea-php74 /opt/cpanel/composer/bin/composer"
		],
		"php80": [
			"/usr/local/bin/ea-php80 /opt/cpanel/composer/bin/composer"
		],
		"teste": [
			"phpunit"
		],
		"testePHP80": [
			"/opt/cpanel/ea-php80/root/usr/bin/php vendor/phpunit/phpunit/phpunit"
		]
    }
}
